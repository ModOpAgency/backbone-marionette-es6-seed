{
  "private": true,
  "engines": {
    "node": ">=0.12.4"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "ignore": [
            "jquery",
            "foundation",
            "modernizr"
          ]
        }
      ],
      [
        "hbsfy",
        {
          "extensions": [
            "hbs"
          ]
        }
      ]
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "modernizr": "./node_modules/foundation-sites/js/vendor/modernizr.js",
    "foundation": "./node_modules/foundation-sites/js/foundation.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "modernizr": "Modernizr",
    "foundation": {
      "exports": "foundation",
      "depends": [
        "modernizr"
      ]
    }
  },
  "dependencies": {
    "backbone": "1.1.2",
    "backbone.marionette": "2.4.1",
    "backbone.radio": "0.9.0",
    "foundation-sites": "5.5.2",
    "handlebars": "1.3.0",
    "jquery": "2.1.3",
    "lodash": "3.7.0",
    "newrelic": "1.16.1",
    "slick-carousel": "1.5.1"
  },
  "devDependencies": {
    "autoprefixer-core": "4.0.2",
    "babelify": "6.1.0",
    "browser-sync": "1.8.2",
    "browserify": "10.1.3",
    "browserify-shim": "3.8.6",
    "css-sprite": "0.9.9",
    "del": "1.1.1",
    "gulp": "3.6.0",
    "gulp-cache": "0.2.2",
    "gulp-csso": "0.2.6",
    "gulp-if": "1.2.1",
    "gulp-imagemin": "2.0.0",
    "gulp-jshint": "1.5.3",
    "gulp-load-plugins": "0.8.0",
    "gulp-minify-html": "0.1.6",
    "gulp-notify": "2.2.0",
    "gulp-postcss": "3.0.0",
    "gulp-sass": "1.3.3",
    "gulp-size": "1.1.0",
    "gulp-sourcemaps": "1.3.0",
    "gulp-uglify": "1.0.1",
    "gulp-uncss": "1.0.1",
    "gulp-useref": "1.0.2",
    "hbsfy": "2.2.1",
    "jshint-stylish": "1.0.0",
    "opn": "1.0.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.2.2"
  }
}
