{
  "private": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "npm run copy && npm run build-webpack",
    "build-webpack": "webpack -p --config webpack.production.config.js",
    "clean": "rm -rf dist",
    "copy": "npm run copy-static && npm run copy-assets",
    "copy-static": "copyfiles -f ./app/index.html ./app/favicon.ico ./dist",
    "copy-assets": "copyfiles -u 1 './app/assets/**' ./dist/",
    "start": "webpack-dev-server"
  },
  "dependencies": {
    "backbone": "1.2.3",
    "backbone-relational": "^0.10.0",
    "backbone.intercept": "^0.4.0",
    "backbone.marionette": "^2.4.3",
    "backbone.radio": "1.0.2",
    "backbone.touch": "^0.4.2",
    "foundation-sites": "5.5.3",
    "i18next": "^2.3.4",
    "i18next-client": "^1.11.4",
    "jquery": "2.1.4",
    "lodash": "^4.6.1",
    "slick-carousel": "1.5.9"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "bluebird": "^3.1.1",
    "color": "^0.11.1",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.1",
    "cssesc": "^0.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "handlebars": "^4.0.2",
    "handlebars-loader": "^1.1.4",
    "httpster": "^1.0.3",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.3",
    "node-libs-browser": "^1.0.0",
    "node.extend": "^1.1.5",
    "parse-filepath": "^0.6.3",
    "postcss-loader": "^0.8.1",
    "prettydiff": "^1.13.5",
    "sass-loader": "^3.0.0",
    "sasslint-loader": "0.0.1",
    "scss-loader": "0.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "webpack-spritesmith": "^0.0.6"
  }
}
